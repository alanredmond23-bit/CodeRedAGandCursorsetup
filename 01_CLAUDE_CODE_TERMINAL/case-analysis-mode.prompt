### 3. OUTCOME PREDICTION MODEL

```python
def predict_case_outcome(
    facts: dict,
    legal_theories: dict,
    jurisdiction: str,
    judge: str = None
) -> dict:
    """
    Predict case outcome using multi-factor analysis
    """

    # Factor 1: Legal Strength
    legal_strength = assess_legal_strength(legal_theories)

    # Factor 2: Factual Strength
    factual_strength = assess_factual_strength(facts)

    # Factor 3: Procedural Posture
    procedural_factors = assess_procedural_posture()

    # Factor 4: Judicial Tendencies (if judge known)
    judicial_factors = assess_judicial_tendencies(judge, jurisdiction)

    # Factor 5: Settlement Dynamics
    settlement_factors = assess_settlement_likelihood()

    # Bayesian probability calculation
    base_rate = 0.50  # Base rate for plaintiff success in contract cases

    # Adjust for legal strength
    if legal_strength["score"] > 0.7:
        base_rate *= 1.3
    elif legal_strength["score"] < 0.4:
        base_rate *= 0.7

    # Adjust for factual strength
    if factual_strength["score"] > 0.7:
        base_rate *= 1.2
    elif factual_strength["score"] < 0.4:
        base_rate *= 0.8

    # Adjust for judicial tendencies
    base_rate *= judicial_factors["adjustment_multiplier"]

    # Cap at 95% (never absolute certainty)
    predicted_probability = min(base_rate, 0.95)

    return {
        "outcome_prediction": {
            "success_probability": predicted_probability,
            "confidence_interval": (predicted_probability - 0.15, predicted_probability + 0.15),
            "confidence_level": "MEDIUM-HIGH",
            "methodology": "Multi-factor Bayesian analysis"
        },
        "contributing_factors": {
            "legal_strength": legal_strength,
            "factual_strength": factual_strength,
            "procedural_posture": procedural_factors,
            "judicial_tendencies": judicial_factors,
            "settlement_dynamics": settlement_factors
        },
        "likely_outcome": determine_likely_outcome(predicted_probability),
        "alternative_scenarios": generate_alternative_scenarios(),
        "key_uncertainties": identify_key_uncertainties()
    }

